{
  "name": "WearableHub",
  "description": "WearableHub NativeScript Application",
  "private": true,
  "main": "main.js",
  "version": "0.1.0",
  "license": "MIT",
  "readme": "NativeScript Application",
  "repository": "<fill-your-repository-here>",
  "nativescript": {
    "id": "org.nativescript.WearableHub",
    "tns-ios": {
      "version": "2.5.0"
    },
    "tns-android": {
      "version": "2.5.0"
    }
  },
  "android": {
    "v8Flags": "--expose_gc"
  },
  "dependencies": {
    "@angular/common": "~2.4.8",
    "@angular/compiler": "~2.4.8",
    "@angular/core": "~2.4.8",
    "@angular/forms": "~2.4.8",
    "@angular/http": "~2.4.8",
    "@angular/platform-browser": "~2.4.8",
    "@angular/platform-browser-dynamic": "~2.4.8",
    "@angular/router": "~3.4.8",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "angular-in-memory-web-api": "^0.2.4",
    "angular2-jwt": "^0.1.28",
    "angular2-moment": "^1.2.0",
    "nativescript-angular": "~1.4.0",
    "nativescript-telerik-ui-pro": "next",
    "nativescript-theme-core": "^1.0.2",
    "ng2-translate": "^5.0.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "~5.0.1",
    "tns-core-modules": "~2.5.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.4.8",
    "@angular/language-service": "~2.4.8",
    "@ngtools/webpack": "~1.2.2",
    "babel-traverse": "6.23.1",
    "babel-types": "6.23.0",
    "babylon": "6.15.0",
    "codelyzer": "2.0.0-beta.4",
    "copy-webpack-plugin": "~4.0.1",
    "extract-text-webpack-plugin": "~2.0.0-beta.4",
    "htmlparser2": "~3.9.2",
    "lazy": "1.0.11",
    "nativescript-css-loader": "~0.26.0",
    "nativescript-dev-android-snapshot": "^0.*.*",
    "nativescript-dev-sass": "^0.4.2",
    "nativescript-dev-typescript": "0.3.x",
    "nativescript-dev-webpack": "0.3.3",
    "raw-loader": "~0.5.1",
    "resolve-url-loader": "~2.0.0",
    "sass-loader": "6.0.x",
    "tslint": "^4.3.0",
    "typescript": "~2.1.6",
    "webpack": "~2.2.1",
    "webpack-sources": "~0.1.3"
  },
  "scripts": {
    "debug-ios-bundle": "tns debug ios --chrome --bundle --disable-npm-install",
    "clean-android": "tns clean-app android",
    "clean-ios": "tns clean-app ios",
    "prewebpack-android": "npm run clean-android",
    "prewebpack-ios": "npm run clean-ios",
    "webpack-android": "webpack --config=webpack.android.js --progress",
    "webpack-ios": "webpack --config=webpack.ios.js --progress",
    "prestart-android-bundle": "npm run webpack-android",
    "prestart-ios-bundle": "npm run webpack-ios",
    "start-android-bundle": "tns run android --bundle --disable-npm-install",
    "start-ios-bundle": "tns run ios --bundle --disable-npm-install",
    "prebuild-android-bundle": "npm run webpack-android",
    "prebuild-ios-bundle": "npm run webpack-ios",
    "build-android-bundle": "tns build android --bundle --disable-npm-install",
    "build-ios-bundle": "tns build ios --bundle --disable-npm-install"
  }
}
